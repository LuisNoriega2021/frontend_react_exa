{
	"name": "app_control_asegurado",
	"version": "1.0.0",
	"description": "Plantilla de creación de aplicación con React, Node, Express, TypeScript, Webpack y comentario para validar cambio",
	"author": "Asesuisa Dev <delias@sesuisa.com>",
	"license": "ISC",
	"main": "src/ServerApp.js",
	"scripts": {
		"start": "node src/ServerApp.js",
		"dev": "SET NODE_ENV=local&& nodemon",
		"webpack": "webpack --mode development --watch",
		"build-ts": "tsc",
		"build-react": "webpack --mode development",
		"release-dev": "node build.js modo=development",
		"release-test": "node build.js modo=test",
		"release-pro": "node build.js modo=production",
		"react-webpack-build": "SET NODE_ENV=production && webpack --config ./webpack.config.ts",
		"lint": "tsc --noEmit && eslint */**/*.{js,ts,tsx} --quiet --fix",
		"test-mocha": "mocha -r ts-node/register tests/**/*.test.ts  -r dotenv/config --timeout 10000 --exit",
		"only-test-mocha": "SET NODE_ENV=local && mocha -r ts-node/register tests/**/*.test.ts --timeout 10000 --exit",
		"coverage": "nyc npm run test-mocha",
		"coverage-reporte": "SET NODE_ENV=local&& npm run coverage",
		"only-sonar": "node sonar-scanner.js",
		"ci-local": "SET NODE_ENV=local&& npm run lint && npm run only-sonar",
		"ci-cloud": "SET NODE_ENV=development&& npm run lint && npm run coverage && npm run only-sonar"
	},
